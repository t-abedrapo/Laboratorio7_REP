    {
      "cell_type": "code",
      "execution_count": 151,
      "metadata": {},
      "outputs": [],
      "source": [
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.pipeline import Pipeline\n",
        "from sklearn.compose import ColumnTransformer\n",
        "from sklearn.preprocessing import StandardScaler, OneHotEncoder\n",
        "from xgboost import XGBClassifier\n",
        "from lightgbm import LGBMClassifier"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 152,
      "metadata": {
        "id": "lLtlXGTPdWAV"
      },
      "outputs": [],
      "source": [
        "#1.\n",
        "re = 2\n",
        "test_size = 0.2\n",
        "\n",
        "y = df['label']\n",
        "X = df.drop(['label' , 'Name' , 'National_Position'], axis = 1)\n",
        "\n",
        "X_train, X_test, y_train, y_test = train_test_split(X, y, \n",
        "                                                    test_size=test_size, \n",
        "                                                    stratify=y,\n",
        "                                                     random_state=re)\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 153,
      "metadata": {},
      "outputs": [],
      "source": [
        "#2.\n",
        "\n",
        "pipeline_xgboost = Pipeline(steps=[\n",
        "    ('preprocesamiento', col_transformer),\n",
        "    ('clasificador', XGBClassifier())\n",
        "])\n",
        "\n",
        "pipeline_lightgbm = Pipeline(steps=[\n",
        "    ('preprocesamiento', col_transformer),\n",
        "    ('clasificador', LGBMClassifier())\n",
        "])"
      ]
    },
